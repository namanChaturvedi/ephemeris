# Default chain ID (0x539 for localhost, 0x61 for BSC Testnet, 0x38 for BSC Mainnet)
REACT_APP_CHAIN_ID=0x539

# Infura or Alchemy API key (optional, for connecting to networks other than localhost)
REACT_APP_INFURA_KEY=your_infura_key_here
REACT_APP_ALCHEMY_KEY=your_alchemy_key_here

# Optional: Analytics tracking ID
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id_here 